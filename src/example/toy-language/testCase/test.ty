extension function toString(this int v):string{
    var baseCode="0".buffer[0];
    if(v>=0&&v<=9){
        return new string({[(byte)v+baseCode]});
    }else if(v>=10&&v<=19){
        return new string({[(byte)(v-10)+baseCode]});
    }else{
        return new string({[(byte)(v-20)+baseCode]});
    }
}


